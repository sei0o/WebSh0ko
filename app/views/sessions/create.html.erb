<% provide :title, "ログイン" %>
<% provide :pagemida, "ログイン" %>
<%= form_for :session, url: { controller: "sessions", action: "_create" } do |f| %>
  <%= f.label :account %>
  <%= f.text_field :account %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
	
  <%= f.submit "ログイン", style: "margin-left:150px;width:300px;" %>
<% end %>
<div style="margin-left:150px;margin-top:15px;">または<%= link_to "ユーザー登録", user_signup_path %></div>